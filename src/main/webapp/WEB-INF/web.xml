<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>  
    <description></description>  
    <display-name>LoginServlet</display-name>  
    <servlet-name>login</servlet-name>  
    <servlet-class>com.trainingproject.servlets.LogInServlet</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>login</servlet-name>  
    <url-pattern>/login</url-pattern>  
  </servlet-mapping>  
  <servlet>  
    <description></description>  
    <display-name>LogoutServlet</display-name>  
    <servlet-name>logout</servlet-name>  
    <servlet-class>com.trainingproject.servlets.LogOutServlet</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>logout</servlet-name>  
    <url-pattern>/logout</url-pattern>  
  </servlet-mapping>  
 <servlet>  
    <description></description>  
    <display-name>RegisterUserServlet</display-name>  
    <servlet-name>register</servlet-name>  
    <servlet-class>com.trainingproject.servlets.RegisterUserServlet</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>register</servlet-name>  
    <url-pattern>/register</url-pattern>  
  </servlet-mapping>
   <servlet>  
    <description></description>  
    <display-name>CheckToken</display-name>  
    <servlet-name>checktoken</servlet-name>  
    <servlet-class>com.trainingproject.servlets.CheckToken</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>checktoken</servlet-name>  
    <url-pattern>/checktoken</url-pattern>  
  </servlet-mapping>
   <servlet>  
    <description></description>  
    <display-name>RegisterAIServlet</display-name>  
    <servlet-name>registerai</servlet-name>  
    <servlet-class>com.trainingproject.servlets.RegisterAIServlet</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>registerai</servlet-name>  
    <url-pattern>/registerai</url-pattern>  
  </servlet-mapping>
   <servlet>  
    <description></description>  
    <display-name>DeleteUserServlet</display-name>  
    <servlet-name>deleteuser</servlet-name>  
    <servlet-class>com.trainingproject.servlets.DeleteUserServlet</servlet-class>  
  </servlet>  
  <servlet-mapping>  
    <servlet-name>deleteuser</servlet-name>  
    <url-pattern>/deleteuser</url-pattern>  
  </servlet-mapping>  
</web-app>